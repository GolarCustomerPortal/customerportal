<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
          "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="com.customerportal.bean.CustomAttachments" table="custom_attachments__c">
  <id name="id">
   <column name="Id"  />
  </id>
  
  <property name="facility" column="Facility__c"/>
   <property name="type" column="Type__c"/>
  <property name="g360URL" column="G360_URL__c"/>	
  <property name="source" column="Source__c"/>
  <property name="description" column="Description__c"/>
  <property name="name" column="Name"/>
  <property name="reportDatae" column="Report_Date__c"/>
 <property name="active" column="Active__c"/>
 </class>
</hibernate-mapping>