<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
          "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.customerportal.bean.SiteIncome"
	table="site_income__c">
		<id name="id"  column="Custom_Id__c">
			<generator class="increment"></generator> 
		</id>
		<property name="accountID" column="Account_ID__c" />
		<property name="fID" column="FID__c" />
		<property name="fromDate" column="From_Date__c" />
		<property name="gallonsSold" column="Gallons_Sold__c" />
		<property name="gasAmount" column="Gas_Amount__c" />
		<property name="insideSalesAmount"
			column="Inside_Sales_Amount__c" />
		<property name="lotteryAmount" column="Lottery_Amount__c" />
		<property name="scratchOffSold" column="Scratch_Off_Sold__c" />
		<property name="toDate" column="To_Date__c" />
		<property name="tax" column="Tax__c" />
		<property name="salesforceId" column="Sales_Force_Id__c" />
		<property name="dataEnteredBy" column="Data_Entered_By__c" />
		<property name="dataModifiedBy" column="Data_Modified_By__c" />
		<property name="createdDate" column="Created_Date__c" />
		<property name="modifiedDate" column="Modified_Date__c" />

	</class>
</hibernate-mapping>